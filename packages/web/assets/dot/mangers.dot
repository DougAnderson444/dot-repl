digraph "ManagersTemplate" {
    // ---------- Global graph attributes ----------
    graph [
        rankdir=LR,
        bgcolor="#fdfdfd",
        labelloc=top,
        label="Manager's Template",
        fontname="Mono",
        fontsize=16,
        pencolor="#666",
        charset="UTF-8"
    ];

    // Default node attributes
    node [
        shape=ellipse,
        fontname="Helvetica",
        fontsize=12,
        style=filled,
        fillcolor="#D7F5D3",
        color="#A5E1A6"
    ];

    // Default edge attributes
    edge [
        color="#666666",
        arrowsize=0.9,
        fontname="Helvetica",
        fontsize=10
    ];


